{
  "name": "mt-pc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node tools/start.js",
    "build": "cross-env CLIENT=main NODE_ENV=production webpack --config config/webpack.config.prod.js --trace-deprecation --hide-modules --display-optimization-bailout --profile --json > compilation-stats.json",
    "build:serve": "npm run build && npm run serve",
    "test": "cross-env NODE_ENV=test  jest --config config/jest.config.js --bail",
    "test:watch": "cross-env NODE_ENV=test  jest --config config/jest.config.js --watch",
    "eject": "react-scripts eject",
    "story": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "precommit": "lint-staged",
    "mock": "json-server --watch ./mock/db.json -p 3001 --nc",
    "build-auto": "cross-env NODE_ENV=production CLIENT=$npm_config_client webpack --config config/webpack.config.prod.js --trace-deprecation --hide-modules --display-optimization-bailout --profile --json > compilation-stats.json"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint -f pretty --fix",
      "prettier --write",
      "git add"
    ],
    "*.{png,jpeg,jpg,svg}": [
      "imagemin-lint-staged",
      "git add"
    ]
  },
  "proxy": {
    "/api": {
      "target": "https://api.github.com/",
      "changeOrigin": true,
      "ws": true
    }
  },
